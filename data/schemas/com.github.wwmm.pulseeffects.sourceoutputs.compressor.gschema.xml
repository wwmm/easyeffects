<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema
        id="com.github.wwmm.pulseeffects.sourceoutputs.compressor"
        path="/com/github/wwmm/pulseeffects/sourceoutputs/compressor/">
        <key name="state" type="b">
            <default>false</default>
        </key>
        <key name="installed" type="b">
            <default>true</default>
        </key>
        <key name="post-messages" type="b">
            <default>false</default>
        </key>
        <key
            name="detection"
            enum="com.github.wwmm.pulseeffects.compressor.detection.enum">
            <default>"RMS"</default>
        </key>
        <key
            name="stereo-link"
            enum="com.github.wwmm.pulseeffects.compressor.stereo.link.enum">
            <default>"Average"</default>
        </key>
        <key name="mix" type="d">
            <range min="-24.0" max="0"/>
            <default>0.0</default>
        </key>
        <key name="attack" type="d">
            <range min="0.01" max="2000"/>
            <default>20.0</default>
        </key>
        <key name="release" type="d">
            <range min="0.01" max="2000"/>
            <default>250.0</default>
        </key>
        <key name="threshold" type="d">
            <range min="-60.0" max="0"/>
            <default>-18</default>
        </key>
        <key name="ratio" type="d">
            <range min="1" max="20"/>
            <default>2</default>
        </key>
        <key name="knee" type="d">
            <range min="0" max="18"/>
            <default>9</default>
        </key>
        <key name="makeup" type="d">
            <range min="0" max="36"/>
            <default>0.0</default>
        </key>
    </schema>
</schemalist>